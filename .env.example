# API Keys - Copy this file to .env and fill in your keys

# Tavily API Key (required for web search)
# Get your key at: https://tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here

# Optional: LangSmith for tracing (optional)
# Get your key at: https://smith.langchain.com/
LANGSMITH_TRACING="false"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT="presentation-agent"

# Optional: Additional LLM providers (if not using OpenRouter)
# GEMINI_API_KEY=your_gemini_api_key_here
# GROQ_API_KEY=your_groq_api_key_here
# OPENROUTER_API_KEY=your_openrouter_api_key_here
